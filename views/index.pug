extends layout

block content
  h1= title
  p Hello Guys  #{title}

  button(type="button", class="btn btn-info" onclick="goFaster()") Press Me

  #faster 
    | Increase the speed

  script. 
    var mph=20;
    var change=1;
    function goFaster()
    {
      mph=mph+change;
      change++;
      document.getElementById("faster").innerHTML=`MPH ${mph} with change ${change}`;
    }
  
  

